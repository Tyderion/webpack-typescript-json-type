{
  "name": "webpack-typescript-json-type",
  "version": "1.0.0",
  "description": "Generates a type with all json paths for the configured files",
  "homepage": "",
  "author": {
    "name": "Gabriel Nadler",
    "email": "nadlergabriel@gmail.com",
    "url": ""
  },
  "files": [
    "lib", "dist"
  ],
  "main": "dist/index.js",
  "keywords": [
    "typescript",
    "webpack",
    "i18n"
  ],
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "typescript": "^2.6.2"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Tyderion/webpack-typescript-json-type.git"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node": "^9.4.0",
    "tslib": "^1.9.0"
  }
}
